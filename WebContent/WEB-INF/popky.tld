	<tag>
		<description>Tag pro vypsani seznamu napr. ve strankovanem seznamu, pokud je seznam delsi nez maximalni 
    		povolena delka, pak se orizne, doplni tremi teckami a cely seznam se da do titulku</description>
		<name>longList</name>
		<tag-class>com.popkyss.sweetShop.setting.ltd.LongList</tag-class>
		<body-content>empty</body-content>

		<attribute>
			<description>hodnota seznamu</description>
			<name>value</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.util.List</type>
		</attribute>

		<attribute>
			<description>maximalni delka seznamu</description>
			<name>maxItem</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Integer</type>
