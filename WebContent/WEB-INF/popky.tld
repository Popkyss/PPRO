	<!-- -**************** BARVA STAVU ****************** -->
	<tag>
		<description>Tag, ktery generuje barevny ctverecek</description>
		<name>barvaStav</name>
		<tag-class>com.popkyss.sweetShop.setting.ltd.StavTag</tag-class>
		<body-content>empty</body-content>
		<attribute>
			<description>Barva stavu.</description>
			<name>barva</name>
			<required>yes</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.Object</type>
		</attribute>
		<attribute>
			<description>Titulek ktery bude k barve stavu prirazen.</description>
			<name>title</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
			<type>java.lang.String</type>
		</attribute>
		<attribute>
			<description>Id span elementu.</description>
			<name>id</name>
			<required>no</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<description>Klasa, podle ktere se bude span element chovat, musi byt definovana v pripojenem css souboru.
